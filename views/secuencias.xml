<odoo>
   <data>
        <record id="compra_servicio_id" model="ir.sequence">
            <field name="name">Compra servicio</field>
            <field name="code">compra.servicio</field>
            <field name="active">TRUE</field>
            <field name="prefix">C</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="movimiento_servicio_id" model="ir.sequence">
            <field name="name">Movimiento servicio</field>
            <field name="code">movimiento.servicio</field>
            <field name="active">TRUE</field>
            <field name="prefix">M</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="inventario_servicio_id" model="ir.sequence">
            <field name="name">Inventario servicio</field>
            <field name="code">inventario.servicio</field>
            <field name="active">TRUE</field>
            <field name="prefix">P</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data> 
</odoo>
